{
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": ["**/__tests__/**/*.ts", "**/?(*.)+(spec|test).ts"],
    "moduleFileExtensions": ["ts", "js", "json", "node"],
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coverageReporters": ["json", "lcov", "text", "clover"]
  }
  